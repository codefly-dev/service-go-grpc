version: v1
managed:
  enabled: true
  go_package_prefix:
    default: {{.Domain}}/pkg/gen
    except:
      - buf.build/googleapis/googleapis
plugins:
  - plugin: buf.build/protocolbuffers/go
    out: ../pkg/gen
    opt: paths=source_relative
  - plugin: buf.build/grpc/go
    out: ../pkg/gen
    opt: paths=source_relative
  - name: grpc-gateway
    out: ../pkg/gen
    opt:
      - paths=source_relative
  - name: openapiv2
    out: swagger
